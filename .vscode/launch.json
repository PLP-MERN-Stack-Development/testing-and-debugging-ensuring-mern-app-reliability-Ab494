{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Tests (npm run test:debug)",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "test:debug"],
      "port": 9229,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Attach to Node (9229)",
      "type": "pwa-node",
      "request": "attach",
      "port": 9229,
      "protocol": "inspector"
    }
  ]
}
